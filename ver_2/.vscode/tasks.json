{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/Fo${workspaceFolder}\\temp\\",
                "/Fe${workspaceFolder}\\bin\\main.exe",
                "${workspaceFolder}\\src\\*.cpp",
                "${workspaceFolder}\\Dependencies\\sFoundation20\\win32\\sFoundation20.lib",
                "${workspaceFolder}\\Dependencies\\TcAdsDll\\Lib\\TcAdsDll.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build test file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/Fo${workspaceFolder}\\temp\\",
                "/Fe${workspaceFolder}\\bin\\${fileBasenameNoExtension}.exe",
                "${workspaceFolder}\\test\\*.cpp",
                "${workspaceFolder}\\Dependencies\\sFoundation20\\win32\\sFoundation20.lib",
                "${workspaceFolder}\\Dependencies\\TcAdsDll\\Lib\\TcAdsDll.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
    ]
}